{
  "name": "web-lib-starter",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build:lib": "pnpm -F '!example*' run build",
    "release": "pnpm -F '^[^example]' publish --access public",
    "test": "pnpm -F '!example*' run test",
    "lint": "eslint . --fix"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "json": "^11.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vitest": "^0.33.0"
  }
}
